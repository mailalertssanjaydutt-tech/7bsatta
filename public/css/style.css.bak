@keyframes blinker {
    0% {
        opacity: 1
    }

    to {
        opacity: 0
    }
}

.blink {
    animation-delay: 5s;
    animation-direction: alternate;
    animation-duration: .6s;
    animation-iteration-count: infinite;
    animation-name: blinker;
    animation-timing-function: ease-in-out;
    font-size: 3rem;
    font-weight: 700;
    -webkit-text-decoration: blink;
    text-decoration: blink
}


